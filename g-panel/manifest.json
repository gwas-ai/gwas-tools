{
  "manifest_version": 3,
  "name": "Mini Spreadsheet",
  "version": "1.0",
  "description": "A functional mini spreadsheet in a Chrome side panel",
  "permissions": [
    "sidePanel",
    "storage"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "Mini Spreadsheet"
  }
}
